{
  "name": "asumranphan/wordpress-sentry-plugin",
  "description": "A Wordpress Plugin send php errors to the Sentry errors reporting system.",
  "type": "wordpress-plugin",
  "license": "GPL-3.0",
  "authors": [{
    "name": "Anurak Sumranphan",
    "email": "a.sumranphan@gmail.com"
  }],
  "require": {
    "php": "^5.6"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.8",
    "wp-coding-standards/wpcs": "^0.11.0"
  },
  "scripts": {
    "phpcs": [
      "./vendor/bin/phpcs --config-set installed_paths vendor/wp-coding-standards/wpcs",
      "./vendor/bin/phpcs -i"
    ]
  }
}
